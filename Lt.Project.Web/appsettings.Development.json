{
  //数据库连接字符串配置
  "ConnectionStrings": {
    //其中SslMode设置为Preferred(如果服务器支持,则使用SSL,但在所有情况下都允许连接)
    "Default": "server=localhost;uid=root;pwd=123456;port=3306;database=test;sslmode=Preferred;"
  },
  //Consul配置
  "ServiceDiscovery": {
    "ServiceName": "longt",
    "BindAddress": "localhost",
    "Port": 5000,
    "Consul": {
      "Address": "http://127.0.0.1:8500",
      "CheckUrl": "/api/Health/Check",
      "DataCenter": "longt",
      "DnsEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  },
  //Redis配置
  "Redis": {
    "HostName": "127.0.0.1",
    "Port": "6379",
    "Password": "123456",
    "Enabled": true
  }
}
